@{
    ViewBag.Title = "Home Pagessss";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Dutch Title: @ViewBag.Title</title>
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
    <header>
        <h2>Welcome to Dutch Treat</h2>
        <menu>
            <ul>
                <li><a asp-controller="App" asp-action="Index">Home</a></li>
                <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
                <li><a asp-controller="App" asp-action="About">About</a></li>
            </ul>
        </menu>
    
    </header>
    <section>
        <h1>@ViewBag.Title</h1>
        @RenderBody()

    </section>

    <footer>Copyright ...</footer>



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/index.js"></script>

</body>
</html>

